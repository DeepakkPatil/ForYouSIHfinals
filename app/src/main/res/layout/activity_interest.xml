        android:layout_alignParentRight="true"
        android:layout_marginTop="55dp"
        android:layout_marginRight="19dp"
        android:background="@drawable/icon_bg"
        android:padding="2dp"
        android:src="@drawable/int_yoga" />

    <ImageView
        android:id="@+id/img7"
        android:layout_width="114dp"
        android:layout_height="119dp"
        android:layout_below="@+id/img4"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="51dp"
        android:background="@drawable/icon_bg"
        android:padding="2dp"
        android:src="@drawable/int_read" />


    <ImageView
        android:id="@+id/img8"
        android:layout_width="114dp"
        android:layout_height="119dp"
        android:layout_below="@+id/img5"
        android:layout_alignParentRight="true"
        android:layout_marginTop="50dp"
        android:layout_marginRight="148dp"
        android:background="@drawable/icon_bg"
        android:padding="2dp"
        android:src="@drawable/int_sports" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="233dp"
        android:layout_height="194dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/salsa"
        android:text="Hello,"

        android:textColor="#000002"
        android:textSize="34sp"
        android:translationZ="10dp" />

    <TextView
        android:id="@id/tvimg4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/img4"
        android:layout_marginLeft="40dp"
        android:fontFamily="@font/salsa"
        android:text="Music"
        android:textColor="@color/black"
        android:textSize="22dp"

        />

    <TextView
        android:id="@+id/tvimg4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/img4"
        android:layout_marginLeft="40dp"
        android:fontFamily="@font/salsa"
        android:text="Music"
        android:textColor="@color/black"
        android:textSize="22dp"

        />

    <TextView
        android:id="@+id/tvimg7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/img7"
        android:layout_marginLeft="35dp"
        android:fontFamily="@font/salsa"
        android:text="Reading"
        android:textColor="@color/black"
        android:textSize="22dp"

        />


    <TextView
