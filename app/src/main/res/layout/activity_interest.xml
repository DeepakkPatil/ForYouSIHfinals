<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Interest"
    >


    <CheckBox
        android:id="@+id/cb1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView2"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="46dp"
        android:layout_marginTop="47dp"
        android:layout_marginRight="313dp"
        android:scaleX="5"
        android:scaleY="5"
        android:textSize="60dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="383dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="60dp"
        android:fontFamily="@font/salsa"
        android:text="Let us know what you like to do!"
        android:textColor="#000002"
        android:textSize="34sp"
        android:translationZ="10dp" />


    <ImageView
        android:id="@+id/img1"
        android:layout_width="114dp"
        android:layout_height="119dp"
        android:layout_below="@+id/textView2"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="27dp"
        android:background="@color/white"
        android:padding="2dp"
        android:src="@drawable/int_workout" />

    <ImageView
        android:id="@+id/img2"
        android:layout_width="114dp"
        android:layout_height="119dp"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="27dp"
        android:background="@drawable/icon_bg"
        android:padding="3dp"
        android:src="@drawable/int_dance" />

    <ImageView
        android:id="@+id/img3"
        android:layout_width="114dp"
        android:layout_height="119dp"
        android:layout_below="@+id/textView2"
        android:layout_alignParentRight="true"
        android:layout_marginTop="27dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/icon_bg"
        android:padding="2dp"
        android:src="@drawable/int_indoor" />

    <ImageView
        android:id="@+id/img4"

        android:layout_width="114dp"
        android:layout_height="119dp"
        android:layout_below="@+id/img1"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="54dp"
        android:background="@drawable/icon_bg"
        android:padding="2dp"
        android:src="@drawable/int_music" />

    <ImageView
        android:id="@+id/img5"
        android:layout_width="114dp"
        android:layout_height="119dp"
